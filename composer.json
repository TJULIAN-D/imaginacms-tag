{
  "name": "imagina/tag-module",
  "description": "Tag management in AsgardCMS",
  "type": "asgard-module",
  "license": "MIT",
  "keywords": [
    "asgardcms",
    "tags",
    "taggable"
  ],
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "phpunit/phpunit": "^10",
    "imagina/page-module": "v10.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Modules\\Tag\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Tag\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev",
      "dev-v10.x": "10.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
